<body ontouchstart="">
    <div class="weui-navbar" style="position: static;">
        <div class="weui-navbar__item weui_bar__item_on">
            <a href="/users/code">我要推广</a>
        </div>
        <div class="weui-navbar__item">
            <a href="/users/customer">已下单的人</a>
        </div>
        <div class="weui-navbar__item">
            <a href="/user/gift">礼物兑换</a>
        </div>
    </div>
    <div>
        <img src="/images/gift.jpg" width="100%" alt="">
        <div class="list-group">
            <% if(data.length> 0){ %>
                <% for(var i = 0;i<data.length;i++ ){ %>
                    <a href="/user/gift/<%= data[i].id %>" class="list-group-item">
                          <img style="width: 100px;" src="<%= data[i].icon_url %>">
                          <%= data[i].title %>
                          <!-- <span class="badge"><%= data[i].consume %></span> -->
                        </a>
                <% } %>
            <% } %>
        </div>
    </div>
</body>

</html>