                <% if(data.length> 0){ %>
                    <% for(var i = 0;i<data.length;i++ ){ %>
                        <a href="/user/gift/<%= data[i].id %>" class="list-group-item">
                          <img style="width: 100px;" src="<%= data[i].icon_url %>">
                          <%= data[i].title %>
                          <!-- <span class="badge"><%= data[i].consume %></span> -->
                        </a>
                    <% } %>
                <% } %>
            </div>
        </div>
